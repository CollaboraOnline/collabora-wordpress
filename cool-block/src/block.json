{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "collabora-wordpress/cool-block",
	"version": "0.1.0",
	"title": "COOL Block",
	"category": "media",
	"description": "Add document collaboration with Collabora Online.",
	"example": {},
	"supports": {
		"color": {
			"background": false,
			"text": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"attributes": {
		"id": {
			"type": "string"
		},
		"mode": {
			"type": "string"
		}
	},
	"textdomain": "collabora-wordpress",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
